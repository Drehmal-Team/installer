{
  "name": "drehmal-installer",
  "version": "0.0.5",
  "description": "Drehmal, Minecraft map installer",
  "productName": "Drehmal Installer",
  "author": "majorlue",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev -m electron --modern",
    "build": "quasar build -m electron",
    "build:debug": "quasar build -m electron --debug",
    "build:win": "quasar build -m electron -T win32",
    "build:linux": "quasar build -m electron -T linux",
    "build:osx": "quasar build -m electron -T mac",
    "build:d:win": "quasar build -m electron -T win32 --debug",
    "build:d:linux": "quasar build -m electron -T linux --debug",
    "build:d:mac": "quasar build -m electron -T mac --debug",
    "clean": "rm -rf node_modules && rm -rf .quasar && rm -rf dist && yarn"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.4",
    "axios": "^1.2.1",
    "dayjs": "^1.11.10",
    "electron-log": "^5.0.3",
    "extract-zip": "^2.0.1",
    "pinia": "^2.0.11",
    "quasar": "^2.14.2",
    "vue": "^3.0.0",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^3.3.1",
    "@quasar/app-vite": "^1.3.0",
    "@types/node": "^12.20.21",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "autoprefixer": "^10.4.2",
    "electron": "^28.1.0",
    "electron-builder": "^24.3.0",
    "electron-packager": "^17.1.1",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^9.0.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
